{
  "name": "audunru/fiken-api-php-client",
  "description": "Connect to the Fiken API",
  "keywords": [
    "fiken"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Audun Rundberg",
      "email": "audun@rundberg.no"
    }
  ],
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "audunru\\FikenClient\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "audunru\\FikenClient\\Tests\\": "tests/"
    }
  },
  "require": {
    "guzzlehttp/guzzle": "^6.3",
    "illuminate/database": "^6.0",
    "illuminate/support": "^6.0",
    "php": "^7.3"
  },
  "require-dev": {
    "dms/phpunit-arraysubset-asserts": "dev-master",
    "mikey179/vfsstream": "^1.6",
    "phpunit/phpunit": "^8.3",
    "symfony/dotenv": "^5.0"
  },
  "scripts": {
    "post-root-package-install": [
      "@php -r \"file_exists('.env.testing') || copy('.env.example', '.env.testing');\""
    ],
    "test": [
      "vendor/bin/phpunit"
    ]
  }
}
